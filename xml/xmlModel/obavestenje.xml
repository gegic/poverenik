<obavestenje vocab="nesto" about="nesto" id="nesto" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xmlSchema/obavestenje.xsd">
    <broj></broj>
    <!-- datum ne mora -->
    <datum>2000-12-12</datum>
    <organ>
        <adresa>
            <mesto></mesto>
            <ulica></ulica>
        </adresa>
        <naziv></naziv>
    </organ>
    <podnosilac id="${zahtev['trazilac-informacije']._attributes.id}">
        <adresa>
            <mesto>${zahtev['trazilac-informacije'].adresa.mesto._text}</mesto>
            <ulica>${zahtev['trazilac-informacije'].adresa.ulica._text}</ulica>
        </adresa>
        <ime-prezime>${zahtev['trazilac-informacije']['ime-prezime']._text}</ime-prezime>
    </podnosilac>
    <opis></opis>
    <zakon>
        <naziv></naziv>
        <clan broj="">
            <stav broj=""/>
        </clan>
    </zakon>
    <zahtev id="${zahtev._attributes.id}">
        <datum>${zahtev.datum._text}</datum>
        <opis-zahteva>${zahtev['opis-zahteva']._text}</opis-zahteva>
    </zahtev>
    <sadrzaj-obavestenja>
        <razlog-odbijanja></razlog-odbijanja>
    </sadrzaj-obavestenja>
    <dodatna-odluka></dodatna-odluka>
    <izdana-dokumenta>
        <uredba>
            <naziv>Uredba o troskovima</naziv>
            <sluzbeni-glasnik>
                <broj>8/06</broj>
            </sluzbeni-glasnik>
        </uredba>
        <cenovnik></cenovnik>
    </izdana-dokumenta>
    <dostavljeno></dostavljeno>
</obavestenje>